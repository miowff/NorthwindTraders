{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ESNext",
    "sourceMap": true,
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@mui/styled-engine": ["./node_modules/@mui/styled-engine-sc"],
      "@/*": ["./src/*"],
      "@backend/*": ["../backend/src/*"]
    }
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./src"
  ]
}
